<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bt.btuzel.storagetestapp">
    <application
        android:name="com.example.storagetestingapp.app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.storagetestingapp.ui.serialize.SerializationActivity"/>
        <activity android:name="com.example.storagetestingapp.ui.img.ImageActivity" />
        <activity android:name=".ui.text.TextActivity" />
        <activity android:name="com.example.storagetestingapp.ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>